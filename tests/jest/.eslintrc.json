{
	"root": true,
	"extends": [
		"wikimedia/client-es6",
		"../../.eslintshared.json"
	],
	"globals": {
		"OO": "readonly"
	},
	"rules": {
		"no-implicit-globals": "off",
		"prefer-const": "off",
		"arrow-parens": "off",
		"no-var": "off",
		"no-undef": "off",
		"no-redeclare": "off",
		"max-len": "off"
	},
	"env": {
		"jest": true
	}
}
